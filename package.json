{
  "name": "decent-league-client",
  "version": "0.4.0",
  "description": "A decent League Client",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --sourcemap",
    "debug": "electron-vite dev --sourcemap --remote-debugging-port=9222",
    "postinstall": "electron-builder install-app-deps",
    "build": "npm run typecheck && electron-vite build",
    "build:unpack": "npm run build && electron-builder --config ./electron-builder.json --dir",
    "build:win": "npm run build && electron-builder --config ./electron-builder.json --win",
    "build:win:publish": "npm run build:win -- --publish always",
    "build:mac": "npm run build && electron-builder --config ./electron-builder.json --mac",
    "build:linux": "npm run build && electron-builder --config ./electron-builder.json --linux",
    "generator": "ts-node generator",
    "g": "npm run generator",
    "lint:main": "biome check --write --error-on-warnings ./src/main",
    "lint:preload": "biome check --write --error-on-warnings ./src/preload",
    "lint:renderer": "biome check --write --error-on-warnings ./src/renderer"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.9.1",
    "cheerio": "^1.1.0",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.6.2",
    "fs-extra": "^11.3.0",
    "league-connect": "^6.0.0-rc13",
    "lodash": "^4.17.21",
    "mime": "^4.0.7",
    "p-limit": "^6.2.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "ts-jenum": "^2.2.2",
    "typeorm": "^0.3.21",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@davstack/store": "^1.4.6",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^4.1.3",
    "@inquirer/prompts": "^7.4.0",
    "@mui/material": "^7.1.1",
    "@optimize-lodash/rollup-plugin": "^5.0.2",
    "@swc/core": "^1.11.13",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.0",
    "@types/node": "^18.19.9",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^35.1.1",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^3.1.0",
    "notistack": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-icons": "^5.0.1",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.23.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-checker": "^0.9.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.2"
  },
  "engines": {
    "node": ">=22.14.0"
  }
}
