{
  "name": "electron-template",
  "version": "1.0.0",
  "description": "An Electron application with React, NestJS and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --sourcemap",
    "debug": "electron-vite dev --sourcemap --remote-debugging-port=9222",
    "postinstall": "electron-builder install-app-deps",
    "build": "npm run typecheck && electron-vite build",
    "build:unpack": "npm run build && electron-builder --config ./electron-builder.json --dir",
    "build:win": "npm run build && electron-builder --config ./electron-builder.json --win",
    "build:mac": "npm run build && electron-builder --config ./electron-builder.json --mac",
    "build:linux": "npm run build && electron-builder --config ./electron-builder.json --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.7",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "better-sqlite3": "^9.4.5",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.3.4",
    "@mui/material": "^5.15.15",
    "@optimize-lodash/rollup-plugin": "^4.0.4",
    "@reduxjs/toolkit": "^2.2.3",
    "@swc/core": "^1.4.12",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.0",
    "@types/node": "^18.19.9",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "apisauce": "^3.0.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-store": "^8.2.0",
    "electron-vite": "^2.3.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.23.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.2",
    "zod": "^3.22.4"
  },
